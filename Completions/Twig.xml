			</behavior>
			<behavior>
				<append> '$[string file]' with {$[data]} %}</append>
			</behavior>
		</completion>
		<completion string="macro" priority="2">
			<behavior suffix="\}">
				<append> $[name]($[arguments]) %}$[]{% endmacro %</append>
			</behavior>
			<behavior suffix="%\}">
				<append> $[name]($[arguments]) %}$[]{% endmacro </append>
			</behavior>
			<behavior suffix="(\s*%\})">
				<append> $[name]($[arguments]) %}$[]{% endmacro</append>
			</behavior>
			<behavior>
				<append> $[name]($[arguments]) %}$[]{% endmacro %}</append>
			</behavior>
		</completion>
		<completion string="macro" priority="1">
