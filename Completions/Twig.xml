			</behavior>
		</completion>
		<completion string="else" />
		<completion string="elseif">
			<behavior suffix="\}">
				<append> $[condition] %</append>
			</behavior>
			<behavior suffix="%\}">
				<append> $[condition] </append>
			</behavior>
			<behavior suffix="(\s*%\})">
				<append> $[condition]</append>
			</behavior>
			<behavior>
