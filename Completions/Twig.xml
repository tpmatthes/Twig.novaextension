			<behavior suffix="%\}">
				<append> '$[string file]' </append>
			</behavior>
			<behavior suffix="(\s*%\})">
				<append> '$[string file]'</append>
			</behavior>
			<behavior>
				<append> '$[string file]' %}</append>
			</behavior>
		</completion>
		<completion string="flush" />
		<completion string="for" priority="2">
			<behavior suffix="\}">
				<append> $[a] in $[b] %}$[]{% endfor %</append>
			</behavior>
			<behavior suffix="%\}">
				<append> $[a] in $[b] %}$[]{% endfor </append>
			</behavior>
			<behavior suffix="(\s*%\})">
				<append> $[a] in $[b] %}$[]{% endfor</append>
			</behavior>
			<behavior>
				<append> $[a] in $[b] %}$[]{% endfor %}</append>
			</behavior>
		</completion>
		<completion string="for" priority="1">
			<behavior suffix="\}">
				<append> $[a] in $[b] %</append>
			</behavior>
			<behavior suffix="%\}">
				<append> $[a] in $[b] </append>
			</behavior>
			<behavior suffix="(\s*%\})">
				<append> $[a] in $[b]</append>
			</behavior>
			<behavior>
				<append> $[a] in $[b] %}</append>
			</behavior>
		</completion>
		<completion string="from">
			<behavior suffix="\}">
				<append> '$[string file]' import $[variable] %</append>
			</behavior>
			<behavior suffix="%\}">
				<append> '$[string file]' import $[variable] </append>
			</behavior>
			<behavior suffix="(\s*%\})">
				<append> '$[string file]' import $[variable]</append>
			</behavior>
			<behavior>
				<append> '$[string file]' import $[variable] %}</append>
			</behavior>
		</completion>
		<completion string="if" priority="2">
			<behavior suffix="\}">
				<append> $[condition] %}$[]{% endif %</append>
			</behavior>
			<behavior suffix="%\}">
				<append> $[condition] %}$[]{% endif </append>
			</behavior>
			<behavior suffix="(\s*%\})">
				<append> $[condition] %}$[]{% endif</append>
			</behavior>
			<behavior>
				<append> $[condition] %}$[]{% endif %}</append>
			</behavior>
		</completion>
		<completion string="if" priority="1">
			<behavior suffix="\}">
